<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:color="@color/mytheme_bg_main" android:state_checked="true">
        <shape android:shape="rectangle">
            <stroke android:width="3px"  android:color="@color/mytheme_bg_main" />
            <corners android:radius="6px"></corners>
        </shape>
    </item>
    <item android:color="@color/mytheme_text_main">
        <shape android:shape="rectangle">
            <stroke android:width="3px" android:color="#f0f0f0" />
            <corners android:radius="6px"></corners>
        </shape>
    </item>

</selector>