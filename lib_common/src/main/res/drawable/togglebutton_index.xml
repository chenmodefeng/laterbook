<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/mytheme_click" />
            <stroke android:width="1dp" android:color="@color/mytheme_text_main4" />
            <corners android:radius="3dp" />

        </shape>
    </item>
    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/new_bg_main" />
            <stroke android:width="1dp" android:color="@color/mytheme_bg_main" />
            <corners android:radius="6px" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/new_bg_main" />
            <stroke android:width="1dp" android:color="@color/mytheme_text_main4" />
            <corners android:radius="6px" />

        </shape>
    </item>
</selector>